{
  "name": "langgraph-python-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:ui\" \"npm run dev:agent\" --names ui,agent --prefix-colors blue,green --kill-others",
    "dev:debug": "LOG_LEVEL=debug npm run dev",
    "dev:agent": "cd agent && uv run main.py",
    "dev:ui": "next dev --webpack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "install:agent": "cd agent && uv sync",
    "postinstall": "npm run install:agent",
    "up": "bun --interactive --latest update"
  },
  "dependencies": {
    "@copilotkit/react-core": "1.51.4",
    "@copilotkit/react-ui": "1.51.4",
    "@copilotkit/runtime": "0.0.0-fix-langgraphhttpagent-loadagentstate-20260220224632",
    "@copilotkitnext/agent": "^1.51.4",
    "@copilotkitnext/runtime": "^1.51.4",
    "next": "16.1.6",
    "pg": "^8.18.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "shiki": "^3.13.0",
    "zod": "^3.24.4"
  },
  "overrides": {
    "@ag-ui/client": "0.0.45",
    "@ag-ui/core": "0.0.45"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.2",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "typescript": "^5"
  }
}